<script>
    $("#voluntryblock").hide();
    // $("#involuntryblock").hide();
    function  hideblock() {



        if($("input[name='assessmentYess']:checked").val()=="voluntry"){
            $("#voluntryblock").show();
            $("#involuntryblock").hide();
        }

        if($("input[name='assessmentYess']:checked").val()=="involuntry"){
            $("#voluntryblock").hide();
            $("#involuntryblock").show();
        }


    }
</script>