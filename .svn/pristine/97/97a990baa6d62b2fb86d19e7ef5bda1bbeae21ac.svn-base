                                            <!--<input id="subcomponentName" name="subcomponentName"-->
                                                   <!--type="text" readonly-->
                                                   <!--ng-model="subcomponentName"-->
                                                   <!--class="form-control input-md">-->
                                        <!--</div>-->
                                    <!--</div>-->
                                    <div class="form-group">
                                        <label class="col-md-4 col-sm-4 control-label" for="">Questions List</label>
                                        <div class="col-md-6 col-sm-6" style="position:relative;">
                                            <select id="subjectId" name="subjectId" type="text"
                                                    ng-model="assessmentDetailsId"    ng-options="class.assessmentDetailsId as
											class.question for class in questionName" class="form-control input-md">
                                                <option value="">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12 admission_border">
                                        <table class="admission_table_1 table">
                                            <tbody>


                                            <tr>
                                                <td colspan="2">
                                                    <table class="table">
                                                        <tr>
                                                            <td>Student</td>
                                                            <td>Max Marks</td>
                                                            <td>Marks</td>
                                                            <td>Remarks</td>
                                                            <tbody>
                                                            <tr ng-repeat="sb in studentMap[assessmentDetailsId]">
                                                                <td>{{sb.studentName}}</td>
                                                                <td>{{sb.maxMarks}}</td>
                                                                <td><input type="text" ng-model="sb.marks" ng-change="validateMarks(sb)">
