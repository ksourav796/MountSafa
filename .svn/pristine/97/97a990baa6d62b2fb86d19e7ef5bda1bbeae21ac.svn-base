                                                <option value="simpleassesment">Simple Assesment</option>
                                                <option value="detailassesment">Detail Assesment</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div ng-show="assessmentYes!='noassesment'&&assessmentYes">
