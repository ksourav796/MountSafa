<div class="container-fluid">
    <div class="container cus_main_row">

        <div class="row">
            <h2 class="pos-header-new">
                <div>Admission</div>
            </h2>
        </div>
        <div class="row inner_row_bg">
            <div class="col-md-12 col-sm-12 col-xs-12">

                <div class="panel">
                    <div class="panel-body">
                        <div class="panel-body" style="height: 65px;">
                            <div class="col-md-4 col-lg-3 col-sm-5 col-xs-5">
                                <form class="form-horizontal ng-pristine ng-valid">
                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <p class="input-group" style="display: block;">
                                                <input id="search_academic" name="textinput" type="text"
                                                       placeholder="Search" ng-model="searchText" class="form-control input-md"
                                                       ng-keyup="$event.keyCode == 13 ? getAdmissionList(searchText) : null"/>
                                            </p>

                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="col-md-9 col-lg-9 col-sm-9 col-xs-7">

                                <div class="form-group">
                                    <div class="col-sm-2">
                                        <button type="button" class="btn btn-primary hi_btn"  ng-click="addNewAdmission()">Add</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="fixed-table-container">
                        <div class="table_wrapfixed-table-container-inner">
                            <div class="header-background"></div>
                            <table data-toggle="table" data-cache="false" data-height="299" class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                                <thead>
                                <tr>
                                    <th data-field="cntryname">
                                        <div class="th-inner">Admin. No</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Image</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Student Name</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">DOB</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Class</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Gender</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Nationality</div></th>
                                    <th data-field="edit">
                                        <div class="th-inner">Actions</div></th>

                                </tr>
                                </thead>
                                <tbody id="parentTable">
                                <input type="hidden" ng-model="studentId" value="{{admissionList.studentId}}"/>
                                <tr ng-repeat="student in admissionList|filter:searchText">
                                    <th>{{$index+1}}</th>
                                    <td><img class="image-preview"
                                             style="width:50px;height:50px; margin-left: 39px;" src={{student.studentPhoto}}></td>
                                    <td>{{student.studentName}}</td>
                                    <td>{{student.dateofbirth}}</td>
                                    <td>{{student.className}}</td>
                                    <td>{{student.gender}}</td>
                                    <td>{{student.nationality}}</td>

                                    <td>
                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="editAdmission(student)">Edit
                                        </button>

                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="viewAdmission(student)">View
                                        </button>
                                        <button type="button" class="btn btn-primary hi_btn"
                                                ng-click="addNewFee(student.acdYearId,student.gradeId,student)">Fee Details
                                        </button>
                                    </td>
                                </tr>
                                <tr ng-if="searchText!=''&&(admissionList|filter:searchText).length==0">
                                    <td colspan="5" style="color: red; font-weight: bold">No Records Found</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="col-sm-12 pagination_">
                            <button type="button" class="btn btn-default hi_btn" ng-class="{'diabled-class': first}"
                                    ng-disabled="first"
                                    ng-click="getAdmissionList('firstPage')" ng-model="first">First
                            </button>
                            <button type="button" class="btn btn-default hi_btn" ng-class="{'diabled-class': prev}"
                                    ng-disabled="prev"
                                    ng-click="getAdmissionList('prevPage')" ng-model="prev">Prev
                            </button>
                            <button type="button" class="btn btn-default hi_btn" name="next" ng-disabled="next"
                                    ng-click="getAdmissionList('nextPage')" value="next"
                                    ng-class="{'diabled-class': next}" ng-model="next">Next
                            </button>
                            <button type="button" class="btn btn-default hi_btn"
                                    ng-click="getAdmissionList('lastPage')" ng-class="{'diabled-class': last}"
                                    ng-disabled="last"
                                    ng-model="last">Last
                            </button>
                        </div>


                    </div>
                </div>
                <div class="modal fade Student" tabindex="-1" id="add_new_fee_modal" role="dialog"
                     data-backdrop="static"
                     aria-labelledby="mySmallModalLabel">
                    <div class="modal-dialog modal-lg" style="width: 1000px !important;">
                        <div class="modal-content checkout">
                            <div class="modal-header">
                                <button type="button" class="close"
                                        data-dismiss="modal" aria-label="Close"><span
                                        aria-hidden="true" ng-click="removeAdmissioDetails()">&times;</span></button>
                                <h4 id="fee-title" class="modal-title" style="color:red;" align="center">
                                    Fee Detials</h4>
                            </div>
                            <div class="table_wrapfixed-table-container-inner">
                                <label class="col-md-4 control-label" for="currency"
                                       style="color:#337ab7" ;>Fee Configuration</label>
                                <div class="header-background"></div>
                                <table data-toggle="table" data-cache="false"
                                       data-height="299"
                                       class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                                    <thead>
                                    <tr>
                                        <th></th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 25%;height: 10% ;font-size: 10px!important;"
                                            data-field="cntryname">
                                            <div class="th-inner">Fee Name</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 11%;height: 10%;font-size: 10px !important;"
                                            data-field="edit">
                                            <div class="th-inner">Fee Amount</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 10px ;height: 10%;font-size: 10px!important;"
                                            data-field="edit">
                                            <div class="th-inner">Discount</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 20% ;height: 10%;font-size: 10px!important;"
                                            data-field="edit">
                                            <div class="th-inner">Discount Type</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="height: 10px ;font-size: 10px!important;"
                                            data-field="edit">
                                            <div class="th-inner">Payable</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="height: 10px ;font-size: 10px!important;"
                                            data-field="edit" data-field="edit">
                                            <div class="th-inner">Installments</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 8px;height: 10%;font-size: 10px !important;"
                                            data-field="edit">
                                            <div class="th-inner">Instalments Amount
                                            </div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 29%;height: 10%;font-size: 10px !important;"
                                            data-field="edit">
                                            <div class="th-inner">Due Date</div>
                                        </th>
                                        <th class="th-inner" data-field="edit"
                                            style="width: 15px;height: 10%;font-size: 10px !important;"
                                            data-field="edit">
                                            <div class="th-inner">Remarks</div>
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody id="parentTable"
                                           ng-repeat="student in selectedFeeList"
                                           class="studentList"
                                           ng-init="sectionIndex = $index">
                                    <tr>
                                        <td>
                                            <div><input id="checkBox" type="checkBox"
                                                        ng-model="student.checkBox"
                                                        ng-disabled="student.paidAmt>0||student.feeTypeStatus==false"
                                                        ng-change="getTotal(student.checkbox)"/>
                                            </div>
                                        </td>
                                        <!--<td hidden>{{student.studentId}}</td>-->
                                        <td colspan="1"
                                            style="width: 15%;height: 10% !important;text-align: left">
                                            {{student.feeTypeName}}
                                        </td>
                                        <td colspan="1"
                                            style="width: 10%;height: 10% !important;text-align: right">
                                            {{student.feeAmount|number:0}}
                                        </td>
                                        <td ng-show="student.checkBox===true"><input
                                                type="text" style="text-align:right"
                                                ng-change="configDiscount(student.discount,$index)"
                                                ng-model-options="{debounce: 1000}"
                                                placeholder="Discount" num-with-space
                                                maxlength="9"
                                                ng-model="student.discount"
                                                ng-disabled="student.paidAmt>0">
                                        </td>
                                        <td ng-show="student.checkBox===false"
                                            style="text-align: right">
                                            {{student.discount}}
                                        </td>
                                        <td><select id="discountId" name="discountId"
                                                    ng-model="student.discountType"
                                                    ng-disabled="student.paidAmt>0" ng-options="discount.discountType as
											                                          discount.discountType for discount in discountTypeList"
                                                    class="form-control input-md">
                                            <option value="">Select</option>
                                        </select>
                                        </td>
                                        <td colspan="1"
                                            style="width: 10%;height: 10% !important;text-align: right">
                                            {{student.payingFee|number:0}}
                                        </td>
                                        <td ng-show="student.value==='true'&&student.checkBox===true">
                                            <input style="text-align:right" type="text"
                                                   ng-change="configDiscount(student.discount,$index)"
                                                   placeholder="No.Of Inst"
                                                   num-with-space maxlength="9"
                                                   ng-model="student.installments"
                                                   ng-disabled="student.paidAmt>0"/>
                                        </td>
                                        <td ng-show="student.value==='false'||student.checkBox===false"
                                            colspan="1"
                                            style="width: 10% ;height: 10%!important;text-align: right">
                                            {{student.installments}}
                                        </td>
                                        <td ng-show="student.value==='true'&&student.checkBox===true">
                                            <input type="text" style="text-align: right"
                                                   placeholder="InstallmentAmount"
                                                   num-with-space maxlength="9"
                                                   ng-change="editSelectedFeeList(student,$index,0)"
                                                   ng-model="student.installmentsPojosList[0].installmentsAmount"
                                                   ng-disabled="student.paidAmt>0">
                                        </td>
                                        <td ng-show="student.value==='false'||student.checkBox===false"
                                            style="text-align: right">
                                            {{student.installmentsPojosList[0].installmentsAmount|number:0}}
                                        </td>
                                        <td ng-show="student.checkBox===true">
                                            <div style="position:relative;">
                                                <p class="input-group">
                                                    <input type="text"
                                                           class="form-control"
                                                           ng-disabled="student.paidAmt>0"
                                                           uib-datepicker-popup="{{format}}"
                                                           ng-change="editSelectedFeeList(student,$index,0)"
                                                           ng-model="student.installmentsPojosList[0].dueDate"
                                                           is-open="$parent.opened[$index]"
                                                           datepicker-options="dateOptions"
                                                           ng-required="false"
                                                           close-text="Close"/>
                                                    <span class="input-group-btn">
                                                                    <button type="button" class="btn btn-default"
                                                                            ng-click="openCustom($event, $index)"
                                                                            ng-disabled="student.paidAmt>0">
                                                                    <i class="glyphicon glyphicon-calendar"></i>
                                                                    </button>
                                                                    </span>
                                                </p>
                                            </div>
                                        </td>
                                        <td ng-show="student.checkBox===false">
                                            {{student.installmentsPojosList[0].dueDate|date:'dd/MM/yyyy'}}
                                        </td>
                                        <td ng-show="student.checkBox===true"><textarea
                                                type="text"
                                                ng-model="student.installmentsPojosList[0].discountRemarks"
                                                maxlength="500"
                                                ng-disabled="student.paidAmt>0"></textarea>
                                        </td>
                                        <td ng-show="student.checkBox===false"><textarea
                                                type="text"
                                                ng-model="student.discountRemarks"
                                                maxlength="500" readonly></textarea>
                                        </td>
                                    </tr>

                                    <tr ng-repeat-start="installment in student.installmentsPojosList"
                                        ng-show="!$first">
                                        <td colspan="7"></td>
                                        <td ng-show="student.checkBox===true"><input
                                                type="text" style="text-align: right"
                                                ng-change="editSelectedFeeList(student,sectionIndex,$index)"
                                                num-with-space
                                                placeholder="InstallmentAmount"
                                                maxlength="9"
                                                ng-model="installment.installmentsAmount"
                                                ng-disabled="student.paidAmt>0">
                                        </td>
                                        <td ng-show="student.checkBox===false">
                                            {{installment.installmentsAmount}}
                                        </td>
                                        <td ng-show="student.checkBox===true">
                                            <div style="position:relative;">
                                                <p class="input-group">
                                                    <input type="text"
                                                           ng-disabled="student.paidAmt>0"
                                                           class="form-control"
                                                           uib-datepicker-popup="{{format}}"
                                                           ng-model="installment.dueDate"
                                                           is-open="student.installmentsPojosList[$index].opened"
                                                           datepicker-options="dateOptions"
                                                           ng-required="false"
                                                           close-text="Close"/>
                                                    <span class="input-group-btn">
                                                                    <button type="button" class="btn btn-default"
                                                                            ng-click="open($event,installment)">
                                                                    <i class="glyphicon glyphicon-calendar"></i></button>
                                                                    </span>
                                                </p>
                                            </div>
                                        </td>
                                        <td><textarea
                                                type="text"
                                                ng-model="installment.discountRemarks"
                                                maxlength="500" ng-disabled="student.paidAmt>0"></textarea>
                                        </td>
                                        <td ng-show="student.checkBox===false">
                                            {{installment.dueDate|date:'dd/MM/yyyy'}}
                                        </td>
                                        <td></td>
                                    <tr ng-repeat-end></tr>
                                    </tr>
                                    </tbody>
                                    <td><strong>Total</strong></td>
                                    <td></td>
                                    <td style="text-align: right"><strong>{{totalFeeValue|number:0}}</strong>
                                    </td>
                                    <td style="text-align: right"><strong>{{totalDiscountAmt|number:0}}</strong>
                                    </td>
                                    <td></td>
                                    <td style="text-align: right"><strong>{{totalPayableAmt|number:0}}</strong>
                                    </td>
                                    <td style="text-align: right"><strong></strong></td>
                                    <td style="text-align: right">
                                        <strong>{{totalInstallmentsAmt|number:0}}</strong>
                                    </td>
                                    <td></td>
                                    <td> </td>

                                </table>
                                </div>
                            <div class="modal-footer" style="border:none !important;">
                                <button type="button"  data-toggle="modal" class="btn btn-success pull-right" ng-click="saveAdmission()">Save</button>


                            </div>

                            </div>


                    </div>


                </div>
                <add-new-admission></add-new-admission>

