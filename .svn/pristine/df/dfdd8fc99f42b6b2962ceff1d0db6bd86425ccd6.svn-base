package com.hyva.bsfms.bs.bsmapper;

import com.hyva.bsfms.bs.bspojo.AcademicYearMasterPojo;
import com.hyva.bsfms.bs.bsentities.AcademicYearMaster;

public class BsAcademicMapper {
    public static AcademicYearMaster mapPojoToEntity(AcademicYearMasterPojo pojo){
        AcademicYearMaster year=new AcademicYearMaster();
        year.setAcdyrDescription(pojo.getAcdyrDescription());
        year.setAcdyrId(pojo.getAcdyrId());
        year.setFromDate(pojo.getFromDate());
        year.setToDate(pojo.getToDate());
        year.setAcdyrName(pojo.getAcdyrName());
        year.setStatus(pojo.getStatus());
        return year;
    }
}
